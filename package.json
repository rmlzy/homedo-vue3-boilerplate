{
  "name": "homedo-vue3-boilerplate",
  "version": "0.0.1",
  "description": "A vue3 admin boilerplate built for homedo.com",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode dev",
    "dev:fat": "vue-cli-service serve --mode fat",
    "dev:uat": "vue-cli-service serve --mode uat",
    "dev:pro": "vue-cli-service serve --mode pro",
    "build:dev": "cross-env NODE_ENV=production vue-cli-service build --mode dev",
    "build:fat": "cross-env NODE_ENV=production vue-cli-service build --mode fat",
    "build:uat": "cross-env NODE_ENV=production vue-cli-service build --mode uat",
    "build:pro": "cross-env NODE_ENV=production vue-cli-service build --mode prod"
  },
  "dependencies": {
    "@vueuse/core": "4.9.0",
    "ant-design-vue": "2.1.2",
    "axios": "0.21.1",
    "dayjs": "1.10.4",
    "lodash": "4.17.21",
    "md5": "2.3.0",
    "mitt": "2.1.0",
    "number-precision": "1.5.0",
    "numbro": "2.3.2",
    "rmb-x": "0.1.2",
    "uuid": "8.3.2",
    "vue": "3.0.11",
    "vue-router": "4.0.6",
    "vuex": "4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.37",
    "@types/webpack-env": "1.16.0",
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-router": "4.5.12",
    "@vue/cli-plugin-typescript": "4.5.12",
    "@vue/cli-plugin-vuex": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "@vue/compiler-sfc": "3.0.11",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-lodash": "3.3.4",
    "compression-webpack-plugin": "5.0.2",
    "core-js": "3.10.1",
    "cross-env": "7.0.3",
    "fork-ts-checker-webpack-plugin": "5.0.11",
    "less": "4.1.1",
    "less-loader": "7.3.0",
    "lodash-webpack-plugin": "0.11.5",
    "prettier": "2.2.1",
    "svg-sprite-loader": "6.0.5",
    "typescript": "4.2.3"
  },
  "homepage": "https://github.com/rmlzy/homedo-vue3-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/rmlzy/homedo-vue3-boilerplate"
  },
  "author": "rmlzy <rmlzy@outlook.com> (https://poppython.com)",
  "license": "MIT"
}
